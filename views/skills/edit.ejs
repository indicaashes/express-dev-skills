<%- include('../partials/header') %>

<form action="/skils/<%= skill.id %>?_method=PUT" method="POST">
  <input type="text" name="skills" value="<%= skill.skill %>"><br>
  <label>
    <input type="checkbox" name="done" <%= skill.done ? 'checked' : '' %>>
    Completed
  </label><br>
  <button type="submit">Update Skill</button>
</form>

<%- include('../partials/footer') %>